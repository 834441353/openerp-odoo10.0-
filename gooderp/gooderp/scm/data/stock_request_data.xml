<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
        <!-- 补货申请序列号 -->
        <record id="seq_stock_request" model="ir.sequence">
            <field name="name">Stock Request</field>
            <field name="code">stock.request</field>
            <field name="prefix">SR%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="auto_reset">True</field>
            <field name="reset_period">day</field>
        </record>
        <!-- 本模块的以下数据表可以清空 -->
        <record id="remove_stock_request" model="business.data.table">
            <field name="name">stock.request</field>
        </record>
        <record id="remove_stock_request_line" model="business.data.table">
            <field name="name">stock.request.line</field>
        </record>
    </data>
</openerp>
