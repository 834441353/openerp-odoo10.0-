<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- 费用报销单序列号 -->
        <record id="seq_hr_expense" model="ir.sequence">
            <field name="name">Expense</field>
            <field name="code">hr.expense</field>
            <field name="prefix">报销%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="auto_reset">True</field>
            <field name="reset_period">day</field>
        </record>
        <record id="seq_hr_expense_line" model="ir.sequence">
            <field name="name">Expense Cost</field>
            <field name="code">hr.expense.line</field>
            <field name="prefix">报销发票%(y)s%(month)s%(day)s</field>
            <field name="padding">2</field>
            <field name="auto_reset">True</field>
            <field name="reset_period">day</field>
        </record>
    </data>
</openerp>
